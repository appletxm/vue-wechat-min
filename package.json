    "eslint-loader": "^3.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-vue": "^5.2.3",
    "express": "^4.17.1",
    "fibers": "^4.0.1",
    "file-loader": "^4.2.0",
    "formidable": "^1.2.1",
    "hf-npm-hooks": "^0.0.3",
    "hf-rules": "^0.0.4",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^0.20.0",
