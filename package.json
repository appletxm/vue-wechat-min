    "version": ">=8.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "hf-rules dirFileName --basePath ./ && hf-rules dirFileName --basePath ./ && hf-rules dirFileName --basePath ./",
      "commit-msg": "hf-rules commitMessage && hf-rules commitMessage && hf-rules commitMessage"
    }
  }
