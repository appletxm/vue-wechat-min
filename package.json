    "jest": "^24.9.0",
    "jest-mock-axios": "^3.1.1",
    "mini-css-extract-plugin": "^0.8.0",
    "multer": "^1.4.2",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "ora": "^3.4.0",
    "postcss-loader": "^3.0.0",
    "rimraf": "^3.0.0",
    "sass": "^1.22.12",
    "sass-loader": "^8.0.0",
    "terser-webpack-plugin": "^2.0.1",
    "url-loader": "^2.1.0",
    "vue-loader": "^14.2.2",
    "vue-template-compiler": "2.6.10",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.10",
    "webpack-dev-middleware": "^3.7.1",
    "webpack-hot-middleware": "^2.25.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/appletxm/hf-vue-h5.git"
  },
  "keywords": [
    "webpack4",
