{
  "name": "vue-wechat-min",
  "version": "1.0.0",
  "private": false,
  "files": [
    "config",
    "dist",
    "mocks",
    "readme-images"
  ],
  "dependencies": {
    "@babel/polyfill": "7.12.1",
    "axios": "0.19.0",
    "hf-web-tool": "0.1.11",
    "lodash": "4.17.15",
    "moment": "2.24.0",
    "registry-webapp": "1.0.4",
    "vant": "2.7.1",
    "vue": "2.6.10",
    "vue-router": "3.0.0",
    "vuex": "3.1.1"
  },
  "scripts": {
    "syncCfg": "node scripts/sync-cfg.js",
    "sync": "node scripts/sync-dependencies.js",
    "clean": "rimraf dist/*",
    "initFiles": "npm run clean && npm run syncModules && npm run syncCfg",
    "mock": "npm run initFiles mock && node scripts/dev-server.js mock",
    "start": "npm run dev",
    "dev": "npm run initFiles development && node scripts/dev-server.js development",
    "build": "npm run initFiles test && node scripts/build.js production test current-branch",
    "buildForProd": "npm run initFiles production && node scripts/build.js production",
    "buildForTest": "npm run initFiles test && node scripts/build.js production test",
    "buildForPre": "npm run initFiles pre && node scripts/build.js production pre",
    "eslint": "eslint --fix --config .eslintrc.js src/js/**/*.{js,vue}",
    "release": "node scripts/release.js production",
    "syncModules": "node scripts/sync-modules.js",
    "debug": "node --inspect scripts/sync-modules.js",
    "test": "jest --coverage",
    "webpack-check": "webpack --config config/webpack.config.check.package.js",
    "postinstall": "hf-npm-hooks postInstall"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.6.0",
